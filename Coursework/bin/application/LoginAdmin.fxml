<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="600.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MainController">
   <children>
      <VBox layoutX="255.0" layoutY="148.0" prefHeight="331.0" prefWidth="266.0">
         <children>
            <TextField fx:id="txtUserNameAdmin" promptText="Admin username" />
            <PasswordField fx:id="txtPasswordAdmin" promptText="Admin Password" />
            <Button fx:id="LoginAdmin" mnemonicParsing="false" onAction="#AdminMenu" text="Login as Admin" />
            <Label fx:id="lblStatusAdmin" text="Status" />
         </children>
      </VBox>
      <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#LoginBack" text="Back" />
   </children>
</AnchorPane>
